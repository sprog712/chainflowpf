{
"compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "outDir": "dist",
  "strict": true,
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "noImplicitAny": true,
  "noImplicitThis": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "strictNullChecks": true,
  "skipLibCheck": true
},
"include": [
  "src/**/*"
],
"exclude": [
  "node_modules",
  "dist"
]
}